            <!--       -->
            <!-- Model -->
            <!--       -->
            
            <!-- Address -->
            <complexType name="Address">
                <sequence>
                    <element name="streetAndNumbers" type="string"/>
                    <element name="street" type="string"/>
                    <element name="houseNumber" type="tns:HouseNumber"/>
                    <element name="apartment" type="string"/>
                    <element name="postalCode" type="string"/>
                    <element name="city" type="string"/>
                    <element name="country" type="tns:Country"/>
					<element name="from" type="string"/>
                </sequence>
            </complexType>

            <!-- AddressParam -->
            <complexType name="AddressParam">
                <sequence>
                    <element name="street" type="string"/>
                    <element name="houseNumber" type="tns:HouseNumber"/>
                    <element name="city" type="string"/>
                </sequence>
            </complexType>

			<!-- Changes -->
            <complexType name="ChangesBase">
                <sequence>
                    <element name="from" type="string"/>
                    <element name="to" type="string"/>
                </sequence>
            </complexType>
			<complexType name="ChangesParam">
				<sequence>
					<element name="from" type="string"/>
					<element name="to" type="string"/>
				</sequence>
			</complexType>

			<!-- CommunityPerson -->
            <complexType name="CommunityPerson">
                <sequence>
                    <element name="status" type="string"/>
                    <element name="person" type="tns:PersonSmall"/>
                    <element name="existingId" type="tns:PrivateId"/>
                </sequence>
            </complexType>

            <!-- CivilStatus -->
            <complexType name="CivilStatus">
                <sequence>
                    <element name="type" type="string"/>
                    <element name="from" type="string"/>
                </sequence>
            </complexType>

            <!-- Country -->
            <complexType name="Country">
                <sequence>
                    <element name="code" type="string"/>
                    <element name="nameFo" type="string"/>
                    <element name="nameEn" type="string"/>
                </sequence>
            </complexType>

            <!-- ForeignSsn -->
            <complexType name="ForeignSsn">
                <sequence>
                    <element name="value" type="string"/>
                    <element name="country" type="tns:Country"/>
                </sequence>
            </complexType>
            <complexType name="ForeignSsnList">
                <sequence>
                    <element name="foreignSsn" type="tns:ForeignSsn" minOccurs="0" maxOccurs="unbounded"/>
                </sequence>
            </complexType>

            <!-- HouseNumber -->
            <complexType name="HouseNumber">
                <sequence>
                    <element name="number" type="int"/>
                    <element name="buildingCode" type="string"/>
                </sequence>
            </complexType>

            <!-- Incapacity -->
            <complexType name="Incapacity">
                <sequence>
                    <element name="guardian1" type="tns:Guardian"/>
                    <element name="guardian2" type="tns:Guardian" minOccurs="0"/>
                </sequence>
            </complexType>

            <!-- Guardian -->
            <complexType name="Guardian">
                <sequence>
                    <element name="name" type="tns:PersonName"/>
                    <element name="address" type="tns:Address"/>
                </sequence>
            </complexType>

            <!-- NameParam -->
            <complexType name="NameParam">
                <sequence>
                    <element name="first" type="string"/>
                    <element name="last" type="string"/>
                </sequence>
            </complexType>

            <!-- NameAndAddressParam -->
            <complexType name="NameAndAddressParam">
                <sequence>
                    <element name="name" type="tns:NameParam"/>
                    <element name="address" type="tns:AddressParam"/>
                </sequence>
            </complexType>
            <complexType name="NameAndAddressParamList">
                <sequence>
                    <element name="param" type="tns:NameAndAddressParam" minOccurs="0" maxOccurs="unbounded"/>
                </sequence>
            </complexType>

            <!-- NameAndDateOfBirthParam -->
            <complexType name="NameAndDateOfBirthParam">
                <sequence>
                    <element name="name" type="tns:NameParam"/>
                    <element name="dateOfBirth" type="string"/>
                </sequence>
            </complexType>
            <complexType name="NameAndDateOfBirthParamList">
                <sequence>
                    <element name="param" type="tns:NameAndDateOfBirthParam" minOccurs="0" maxOccurs="unbounded"/>
                </sequence>
            </complexType>

            <!-- PersonName -->
            <complexType name="PersonName">
                <sequence>
                    <element name="first" type="string"/>
                    <element name="middle" type="string"/>
                    <element name="last" type="string"/>
                    <element name="official" type="string"/>
                </sequence>
            </complexType>

            <!-- PersonSmall -->
            <complexType name="PersonSmall">
                <sequence>
                    <element name="privateId" type="tns:PrivateId"/>
                    <element name="name" type="tns:PersonName"/>
                    <element name="gender" type="string"/>
                    <element name="address" type="tns:Address"/>
                    <element name="placeOfBirth" type="string"/>
                    <element name="incapacity" type="tns:Incapacity"/>
                    <element name="specialMarks" type="tns:SpecialMarks"/>
                    <element name="dateOfDeath" type="string"/>
		        </sequence>
            </complexType>
            
            <!-- PersonMedium -->
            <complexType name="PersonMedium">
                <complexContent>
                    <extension base="tns:PersonSmall">
		                <sequence>
		                    <element name="publicId" type="tns:PublicId"/>
                            <element name="ptal" type="string"/>
                            <element name="dateOfBirth" type="string"/>
		                    <element name="civilStatus" type="tns:CivilStatus"/>
		                </sequence>
		            </extension>
		        </complexContent>
            </complexType>

            <!-- PrivateId -->
            <complexType name="PrivateId">
                <sequence>
                    <element name="value" type="int"/>
                </sequence>
            </complexType>
            <complexType name="PrivateIdList">
                <sequence>
                    <element name="id" type="tns:PrivateId" minOccurs="0" maxOccurs="unbounded"/>
                </sequence>
            </complexType>

            <!-- PrivateChanges -->
            <complexType name="PrivateChanges">
                <complexContent>
                    <extension base="tns:ChangesBase">
		                <sequence>
		                    <element name="ids" type="tns:PrivateIdList"/>
        		        </sequence>
        		    </extension>
        		</complexContent>
